{
  "name": "webdriverio-test-automation",
  "version": "1.0.0",
  "description": "WebdriverIO | JavaScript | Page Object Design Pattern",
  "main": "index.js",
  "scripts": {
    "selenium-server": "./node_modules/.bin/selenium-standalone start",
    "clean": "rm -r allure-results",
    "test": "./node_modules/.bin/wdio wdio.conf.js",
    "test:addReaction": "./node_modules/.bin/wdio wdio.conf.js --spec ./test/TC_01_addReaction.spec.js",
    "test:publishPost": "./node_modules/.bin/wdio wdio.conf.js --spec ./test/TC_02_publishPost.spec.js",
    "test:addNewPage": "./node_modules/.bin/wdio wdio.conf.js --spec ./test/TC_03_addNewPage.spec.js",
    "test-report": "allure serve"
  },
  "author": "Jay Kishore Duvvuri",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/register": "^7.16.0",
    "@wdio/cli": "^6.11.3",
    "@wdio/runner": "^6.11.3",
    "@wdio/selenium-standalone-service": "^6.11.0",
    "allure-commandline": "^2.13.8",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "chai-webdriverio": "^1.0.0",
    "chromedriver": "^87.0.5",
    "mocha": "^9.1.3",
    "wdio-chromedriver-service": "^7.2.2",
    "webdriverio": "^7.16.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/preset-env": "^7.12.11",
    "@wdio/allure-reporter": "^6.11.0",
    "@wdio/local-runner": "^6.11.3",
    "@wdio/mocha-framework": "^7.16.6",
    "@wdio/spec-reporter": "^6.11.0"
  }
}
